<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dogenote"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="18"
        android:targetSdkVersion="22" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/dogenote"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo.Light">
        <activity
            android:name="com.dogenote.dogenote.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.dogenote.aboutlogin.RegisterActivity" 
            android:label="注册">
        </activity>
        <activity android:name="com.dogenote.aboutlogin.LoginActivity" 
            android:label="登陆">
        </activity>
        <activity
            android:name="com.dogenote.extra.noteEdit"
            android:label="编辑" >
        </activity>
    </application>

</manifest>